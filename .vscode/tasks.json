{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "shell",
			"command": "~/.cargo/bin/cargo",
			"args": [
				"run"
			],
			"problemMatcher": [
				"$rustc"
			],
			"label": "cargo run"
		},
		{
			"type": "shell",
			"command": "~/.cargo/bin/cargo",
			"args": [
				"build"
			],
			"problemMatcher": [
				"$rustc"
			],
			"group": "build",
			"label": "cargo build"
		}
	]
}